{
    "name": "neznajki/json-rpc-client-base",
    "description": "Client Base for Json Rpc Requests",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Maris Locmelis",
            "email": "neznajki@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "JsonRpcClientBase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\JsonRpcClientBase\\": "tests/src"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
