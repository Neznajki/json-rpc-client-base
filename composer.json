        "neznajki/json-rpc-common": "^1.1"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:Neznajki/codeception-test-engine.git"
        },
        {
            "type": "git",
            "url": "git@github.com:Neznajki/json-rpc-common.git"
        }
    ],
    "require-dev": {
        "neznajki/codeception-test-engine": "^0.1.1"
    }
}
